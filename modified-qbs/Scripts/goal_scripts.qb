#00000  FUNCTION EndGame DefaultGoalParams
#00000    IF IsCareerMode
#00000      PauseMusic 1
#00000      SwitchOnBoard
#00000      PauseSkaters
#00000      TeleportSkaterDown
#00000      GLOBAL.Goal_Complete_Start_Script

#00000      KillPhotoGuy
#00000      KillGoalMessage id = GoalText
            " "
#00000      KillGoalMessage id = complete
            " "
#00000      PlaySkaterCamAnim skater = 0
            name = GLOBAL.CameraStart
            loop
#00000      IF JustGotGoal goal = GOAL_HIGHSCORE

#00000        PlaySkaterCamAnim skater = 0
              name = GLOBAL.CameraStart

#00000      ELSE
#00000        IF JustGotGoal goal = GOAL_PROSCORE

#00000          PlaySkaterCamAnim skater = 0
                name = GLOBAL.CameraStart

#00000        ELSE
#00000          IF JustGotGoal goal = GOAL_SICKSCORE

#00000            PlaySkaterCamAnim skater = 0
                  name = GLOBAL.CameraStart

#00000          ELSE
#00000            IF JustGotGoal goal = GOAL_SKATE

#00000              PlaySkaterCamAnim skater = 0
                    name = GLOBAL.CameraStart

#00000            ELSE
#00000              IF JustGotGoal goal = GOAL_TAPE

#00000                PlaySkaterCamAnim skater = 0
                      name = GLOBAL.CameraStart

#00000              ELSE
#00000              END IF
#00000            END IF
#00000          END IF
#00000        END IF
#00000      END IF
#00000      IF JustGotGoal goal = GOAL_HIGHSCORE

#00000        SetGlobalFlag flag = PROMPT_FOR_SAVE

#00000        PlaySound goaldone
#00000        LaunchLocalMessage id = GoalText
              GLOBAL.Goal_HighScore_Text
              panel_message_goal
#00000        LaunchLocalMessage id = complete
              "Complete" panel_message_goalcompletemid
#00000        SetSkaterCamAnimSkippable skater = 0
              skippable = 1

#00000        LOOP
#00000          IF SkaterCamAnimFinished skater = 0
                  name = GLOBAL.CameraStart

#00000            BREAK

#00000          END IF
#00000          wait 1 gameframe
#00000        END LOOP 100
#00000      END IF
#00000      Debounce X time = 0.3

#00000      IF JustGotGoal goal = GOAL_PROSCORE

#00000        SetGlobalFlag flag = PROMPT_FOR_SAVE

#00000        PlaySound goaldone
#00000        LaunchLocalMessage id = GoalText
              GLOBAL.Goal_ProScore_Text
              panel_message_goal
#00000        LaunchLocalMessage id = complete
              "Complete" panel_message_goalcompletemid
#00000        SetSkaterCamAnimSkippable skater = 0
              skippable = 1

#00000        LOOP
#00000          IF SkaterCamAnimFinished skater = 0
                  name = GLOBAL.CameraStart

#00000            BREAK

#00000          END IF
#00000          wait 1 gameframe
#00000        END LOOP 100
#00000      END IF
#00000      Debounce X time = 0.3

#00000      IF JustGotGoal goal = GOAL_SICKSCORE

