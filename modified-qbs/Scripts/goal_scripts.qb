              GLOBAL.Goal_SickScore_Text
              panel_message_goal
#00000        LaunchLocalMessage id = complete
              "Complete" panel_message_goalcompletemid
#00000        SetSkaterCamAnimSkippable skater = 0
              skippable = 1

#00000        LOOP
#00000          IF SkaterCamAnimFinished skater = 0
                  name = GLOBAL.CameraStart

#00000            BREAK

#00000          END IF
#00000          wait 1 gameframe
#00000        END LOOP 100
#00000      END IF
#00000      Debounce X time = 0.3

#00000      IF JustGotGoal goal = GOAL_SKATE

#00000        SetGlobalFlag flag = PROMPT_FOR_SAVE

#00000        PlaySound goaldone
#00000        LaunchLocalMessage id = GoalText
              GLOBAL.Goal_SKATE_Text
              panel_message_goal
#00000        LaunchLocalMessage id = complete
              "Complete" panel_message_goalcompletemid
#00000        SetSkaterCamAnimSkippable skater = 0
              skippable = 1

#00000        LOOP
#00000          IF SkaterCamAnimFinished skater = 0
                  name = GLOBAL.CameraStart

#00000            BREAK

#00000          END IF
#00000          wait 1 gameframe
#00000        END LOOP 100
#00000      END IF
#00000      Debounce X time = 0.3

#00000      IF JustGotGoal goal = GOAL_TAPE

#00000        SetGlobalFlag flag = PROMPT_FOR_SAVE

